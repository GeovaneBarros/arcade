{
  "base": "base",
  "execute": [
    {
      "action": "load_folder"
    },
    {
      "action": "board",
      "file": "config/board.md"
    },
    {
      "action": "links",
      "dir": ".indexer/links"
    },
    {
      "action": "thumbs",
      "width": 320,
      "height": 180
    },
    {
      "action": "index",
      "intro": "Readme.md",
      "file": "categorias.md",
      "sorting": {
        "orphan": "No category",
        "group_by": "category",
        "sort_by": "fulltitle",
        "reverse_sort": false
      }
    },
    {
      "action": "view",
      "intro": "Readme.md",
      "file": "photo_board.md",
      "sorting": {
        "orphan": "No category",
        "group_by": "category",
        "sort_by": "fulltitle",
        "reverse_sort": false
      },
      "viewing": {
        "posts_per_row": 4,
        "empty_fig": "config/empty.png"
      }
    }
  ]
}